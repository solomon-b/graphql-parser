[ TypeSysDef
    ( DirDef
        ( DirectiveDefinition
            { dirDefDescription = Nothing
            , dirDefName = "example"
            , dirDefArguments = []
            , dirDefLocations =
                [ TypeSysDirLoc TSDLFIELD_DEFINITION
                , TypeSysDirLoc TSDLARGUMENT_DEFINITION
                ]
            }
        )
    )
, TypeSysDef
    ( TypeDef
        ( OTDef
            ( ObjectTypeDefinition
                { objectDescription = Nothing
                , objectName = "SomeType"
                , objectInterfaces = Nothing
                , objectDirectives = []
                , objectFields = Just
                    ( FieldDefinition
                        { fieldDefDescription = Nothing
                        , fieldDefName = "field"
                        , fieldDefArgumentsDef =
                            [ InputValueDefinition
                                { inputValueDescription = Nothing
                                , inputValueName = "arg"
                                , inputValueType = NamedType "Int"
                                , inputValueDefault = Nothing
                                , inputValueDirectives =
                                    [ Directive
                                        { dName = "example"
                                        , dArguments = Nothing
                                        }
                                    ]
                                }
                            ]
                        , fieldDefType = NamedType "String"
                        , fieldDefDirectives =
                            [ Directive
                                { dName = "example"
                                , dArguments = Nothing
                                }
                            ]
                        } :| []
                    )
                }
            )
        )
    )
]